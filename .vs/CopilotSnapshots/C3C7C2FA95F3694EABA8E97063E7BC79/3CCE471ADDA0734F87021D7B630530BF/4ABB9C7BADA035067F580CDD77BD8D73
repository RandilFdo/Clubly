                                                                                            private void InitializeSettingsPanel()

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        


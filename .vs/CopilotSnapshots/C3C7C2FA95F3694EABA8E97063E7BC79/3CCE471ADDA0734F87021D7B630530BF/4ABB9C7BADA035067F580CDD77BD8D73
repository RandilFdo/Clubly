                                                                                                                                        filteredClubs = filteredClubs.OrderByDescending(c => c.UnpaidDues).ToList();

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        


                                                                                                                                        filteredClubs = filteredClubs.OrderBy(c => c.Name).ToList(); // Default sort

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        


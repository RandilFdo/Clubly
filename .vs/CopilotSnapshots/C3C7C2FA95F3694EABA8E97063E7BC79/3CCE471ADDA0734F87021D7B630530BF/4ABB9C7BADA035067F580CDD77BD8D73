                                                                                    var saveButton = new Button { Text = "Save Preferences", Location = new Point(10, 80), Size = new Size(150, 30) };

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        


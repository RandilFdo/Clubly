                                                    private static Bitmap CreateSolidColorBitmap(int width, int height, Color color)
        {
            Bitmap bmp = new Bitmap(width, height);
            using (Graphics g = Graphics.FromImage(bmp))
            {
                g.FillRectangle(new SolidBrush(color), 0, 0, width, height);
            }
            return bmp;
        }

        private Color GetColorForInitial(char initial)
        {
            // Simple hash to color mapping for initials
            int hash = initial % 5; // 5 distinct colors
            switch (hash)
            {
                case 0: return Color.FromArgb(102, 126, 234); // Blueish
                case 1: return Color.FromArgb(234, 102, 126); // Reddish
                case 2: return Color.FromArgb(126, 234, 102); // Greenish
                case 3: return Color.FromArgb(234, 126, 102); // Orangish
                case 4: return Color.FromArgb(102, 234, 126); // Tealish
                default: return Color.Gray;
            }
        }

        private GraphicsPath GetRoundedRect(RectangleF rect, float radius)
                                                    {
                                                        GraphicsPath path = new GraphicsPath();
                                                        path.StartFigure();
                                                        path.AddArc(rect.X, rect.Y, radius * 2, radius * 2, 180, 90);
                                                        path.AddArc(rect.Right - radius * 2, rect.Y, radius * 2, radius * 2, 270, 90);
                                                        path.AddArc(rect.Right - radius * 2, rect.Bottom - radius * 2, radius * 2, radius * 2, 0, 90);
                                                        path.AddArc(rect.X, rect.Bottom - radius * 2, radius * 2, radius * 2, 90, 90);
                                                        path.CloseFigure();
                                                        return path;
                                                    }

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                            
                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                            
                                                            private void InitializeMyCalendarPanel()

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                            
                                                            {

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                            

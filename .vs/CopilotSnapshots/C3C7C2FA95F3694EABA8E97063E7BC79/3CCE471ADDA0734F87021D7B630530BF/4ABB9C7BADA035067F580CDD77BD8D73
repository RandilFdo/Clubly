                                                                                                                                        filteredClubs = filteredClubs.OrderByDescending(c => c.OutstandingTasks).ToList();

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        


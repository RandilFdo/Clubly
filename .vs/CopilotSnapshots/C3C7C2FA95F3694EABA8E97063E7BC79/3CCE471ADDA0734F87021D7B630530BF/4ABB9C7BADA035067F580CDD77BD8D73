                                                                                    var color4 = new Button { BackColor = ColorTranslator.FromHtml("#db2777"), Size = new Size(30, 30), Location = new Point(130, 40), Tag = "#DB2777" }; // Pink

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                                                    panelSettings.Controls.Add(color1);

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

                                                    

        

                    

        

                                

        

                    

        

                            

        

                    

        

                                

        

                    

        

